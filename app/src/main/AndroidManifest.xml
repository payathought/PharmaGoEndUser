<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pharmagoenduser">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon_final"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_final"
        android:supportsRtl="true"
        android:theme="@style/Theme.PharmaGoEndUser">
        <activity android:name=".View.TermsAndPrivacyLandingPage"></activity>
        <activity android:name=".View.TermsAndConditionActivity" />
        <activity android:name=".View.TermsAndPrivacyActivity" />
        <activity android:name=".View.ViewOrderActivity" />
        <activity android:name=".View.MyCartActivity" />
        <activity
            android:name=".View.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.MedicineListActivity" />
        <activity android:name=".OrderMedicineActivity" />
        <activity android:name=".View.DriverHomeActivity" />
        <activity
            android:name=".View.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignUp"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>